<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>TigerOpenAPI</PackageId>
    <Version>1.1.3</Version>
    <Authors>tiger</Authors>
    <Company>TIGER BROKERS</Company>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <NoWarn>CS8618;</NoWarn>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyDescriptionAttribute>false</GenerateAssemblyDescriptionAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <GenerateAssemblyTitleAttribute>false</GenerateAssemblyTitleAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyVersionAttribute>false</GenerateAssemblyVersionAttribute>
    <GenerateAssemblyFileVersionAttribute>false</GenerateAssemblyFileVersionAttribute>
  </PropertyGroup>

  <ItemGroup>
    <None Include="docs/README.md" Pack="true" PackagePath="/" />
    <None Remove="Newtonsoft.Json" />
    <None Remove="NLog" />
    <None Remove="Common\" />
    <None Remove="Common\Util\" />
    <None Remove="BouncyCastle" />
    <None Remove="Common\Enum\" />
    <None Remove="Common\Struct\" />
    <None Remove="Config\" />
    <None Remove="Quote\" />
    <None Remove="Trade\" />
    <None Remove="Model\" />
    <None Remove="Quote\Model\" />
    <None Remove="BouncyCastle.Cryptography" />
    <None Remove="Polly" />
    <None Remove="Quote\Response\" />
    <None Remove="Trade\Model\" />
    <None Remove="Trade\Response\" />
    <None Remove="Push\" />
    <None Remove="Google.Protobuf" />
    <None Remove="Google.Protobuf.Tools" />
    <None Remove="Push\pb\" />
    <None Remove="Push\proto\" />
    <None Remove="Push\Model\" />
    <None Remove="DotNetty.Codecs.Protobuf" />
    <None Remove="DotNetty.Handlers" />
    <None Remove="Properties\" />
    <None Remove="Common\Watch\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="NLog" Version="5.1.0" />
    <PackageReference Include="BouncyCastle.Cryptography" Version="2.0.0" />
    <PackageReference Include="Polly" Version="7.2.3" />
    <PackageReference Include="Google.Protobuf" Version="3.21.12" />
    <PackageReference Include="Google.Protobuf.Tools" Version="3.21.12" />
    <PackageReference Include="DotNetty.Codecs.Protobuf" Version="0.7.5" />
    <PackageReference Include="DotNetty.Handlers" Version="0.7.5" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Common\" />
    <Folder Include="Common\Util\" />
    <Folder Include="Common\Enum\" />
    <Folder Include="Common\Struct\" />
    <Folder Include="Config\" />
    <Folder Include="Quote\" />
    <Folder Include="Trade\" />
    <Folder Include="Model\" />
    <Folder Include="Quote\Model\" />
    <Folder Include="Quote\Response\" />
    <Folder Include="Trade\Model\" />
    <Folder Include="Trade\Response\" />
    <Folder Include="Push\" />
    <Folder Include="Push\pb\" />
    <Folder Include="Push\proto\" />
    <Folder Include="Push\Model\" />
    <Folder Include="Properties\" />
    <Folder Include="Common\Watch\" />
  </ItemGroup>
</Project>
